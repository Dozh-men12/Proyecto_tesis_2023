<?xml version="1.0" encoding="utf-8" ?>
<TabbedPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Proyecto_tesis_2023.View.Tabbed.ContainerTabbedPage"
             xmlns:views="clr-namespace:Proyecto_tesis_2023.View.Tabbed"
             NavigationPage.HasNavigationBar="False"
             BarBackgroundColor="Transparent"
            >
    
    <TabbedPage.Children>
        <NavigationPage Title="Inicio" IconImageSource="icon_home.png">
            <x:Arguments>
                <views:Inicio/>
            </x:Arguments>
        </NavigationPage>

        <NavigationPage Title="Reservar" IconImageSource="search_icon.png">
            <x:Arguments>
                <views:Reservar/>
            </x:Arguments>
        </NavigationPage>

        <NavigationPage Title="Perfil" IconImageSource="user2_icon">
            <x:Arguments>
                <views:Mis_reservas/>
            </x:Arguments>
        </NavigationPage>

        <NavigationPage Title="Normas" IconImageSource="reglamentos_icon.png">
            <x:Arguments>
                <views:Reglamentos/>
            </x:Arguments>
        </NavigationPage>

    </TabbedPage.Children>
</TabbedPage>